{
  "name": "@opendaemon/core",
  "version": "1.0.0",
  "description": "OpenDaemon core micro-kernel",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
